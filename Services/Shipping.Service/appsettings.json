{
  "Logging": {
    "Region": "us-east-1",
    "LogGroup": "SistemaEnvios.Shipments.Service",
    "IncludeLogLevel": true,
    "IncludeCategory": true,
    "IncludeNewline": true,
    "IncludeException": true,
    "IncludeEventId": false,
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft.AspNetCore": "Information"
    }
  },
  "ShipmentsConnection": {
    "Server": "{MONGO_SERVER}",
    "Database": "{MONGO_DATABASE}",
    "Collection": "{MONGO_COLLECTION}"
  },
  "AllowedHosts": "*",
  "ClientsService": "{CLIENTS_SERVICE}",
  "DeliveryPointsService": "{DELIVERY_POINTS_SERVICE}",
  "MinimumLogLevel":  "{LOG_LEVEL}"
}
